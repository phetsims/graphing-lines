{
  "name": "graphing-lines",
  "version": "1.4.3-rc.1",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/graphing-lines.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "GRAPHING_LINES",
    "phetLibs": [
      "vegas"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet"
    ],
    "simFeatures": {
      "supportsSound": true,
      "supportedRegionsAndCultures": [
        "africa",
        "africaModest",
        "asia",
        "latinAmerica",
        "oceania",
        "random",
        "usa"
      ],
      "defaultRegionAndCulture": "random",
      "supportsDynamicLocale": true,
      "colorProfiles": [
        "default"
      ]
    },
    "simulation": true,
    "supportsOutputJS": true,
    "published": true,
    "screenNameKeys": [
      "GRAPHING_LINES/screen.slope",
      "GRAPHING_LINES/screen.slopeIntercept",
      "GRAPHING_LINES/screen.pointSlope",
      "GRAPHING_LINES/screen.lineGame"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "rules": {
          "@typescript-eslint/ban-ts-comment": [
            "error",
            {
              "ts-expect-error": "allow-with-description",
              "ts-ignore": true,
              "ts-check": true,
              "ts-nocheck": true
            }
          ]
        }
      }
    ]
  }
}